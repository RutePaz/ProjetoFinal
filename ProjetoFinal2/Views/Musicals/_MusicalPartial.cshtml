@model IEnumerable<TI2PFINAL.Models.Soundtrack>


<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.SongName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Duration)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SongName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Duration)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit","Soundtracks", new { id = item.ID_Song }, null) |
            @Html.ActionLink("Delete", "Delete", "Soundtracks", new { id=item.ID_Song },null)
        </td>
    </tr>
}

</table>
